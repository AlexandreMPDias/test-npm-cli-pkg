{
	"name": "alex-npm-cli",
	"version": "0.3.2",
	"description": "",
	"main": "./lib/index.js",
	"bin": {
		"alex": "./lib/index.js"
	},
	"files": [
		"lib/**/*"
	],
	"scripts": {
		"mock": "node ./lib/index.js",
		"build": "tsc",
		"test": "echo jest",
		"start": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/index.ts",
		"kreate": "npm run build && npm run test",
		"local": "npm i -g && alex",
		"refresh": "rm -rf ./node_modules ./package-lock.json && npm install",
		"format": "prettier --write \"src/**/*.ts\"",
		"lint": "tslint -p tsconfig.json",
		"prepare": "npm run build",
		"prepublishOnly": "npm test && npm run lint",
		"preversion": "npm run lint",
		"version": "npm run format && git add -A src",
		"postversion": "git push && git push --tags",
		"update:npm": "npm run version && npm publish"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/AlexandreMPDias/test-npm-cli-pkg.git"
	},
	"author": "tijuk",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/AlexandreMPDias/test-npm-cli-pkg/issues"
	},
	"homepage": "https://github.com/AlexandreMPDias/test-npm-cli-pkg#readme",
	"dependencies": {
		"bluebird": "^3.7.2",
		"chalk": "^4.1.0",
		"clear": "^0.1.0",
		"commander": "^6.2.0",
		"figlet": "^1.5.0",
		"figures": "^3.2.0",
		"fs-extra": "^9.0.1",
		"inquirer": "^7.3.3",
		"inversify": "^5.0.1",
		"kleur": "^4.1.3",
		"log-symbols": "^4.0.0",
		"ramda": "^0.27.1",
		"reflect-metadata": "^0.1.13",
		"ts-lib": "^0.0.5",
		"yargs": "^16.1.1"
	},
	"devDependencies": {
		"@liftr/tscov": "^2.0.0",
		"@types/bluebird": "^3.5.33",
		"@types/clear": "^0.1.0",
		"@types/figlet": "^1.2.1",
		"@types/inquirer": "^7.3.1",
		"@types/jest": "^26.0.16",
		"@types/node": "^14.14.7",
		"@types/ramda": "^0.27.32",
		"@types/yargs": "^15.0.9",
		"jest": "^26.6.3",
		"nodemon": "^2.0.6",
		"prettier": "^2.1.2",
		"ts-jest": "^26.4.4",
		"ts-node": "^9.0.0",
		"tslint": "^6.1.3",
		"tslint-config-prettier": "^1.18.0",
		"typedoc": "^0.19.2",
		"typescript": "^4.1.0-beta"
	}
}
