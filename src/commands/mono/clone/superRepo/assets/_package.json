{
	"name": "liber-apps-super-repo",
	"version": "1.0.0-beta",
	"description": "",
	"dependencies": {},
	"devDependencies": {
		"@types/node": "^14.14.7",
		"npm-delay": "^1.0.4",
		"npm-run-all": "^4.1.5",
		"typescript": "^4.0.5"
	},
	"scripts": {
		"---- all ----------": "echo ---- All  Scripts -----",
		"checkout:dev.all": "run-s checkout:dev.native checkout:dev.containers checkout:dev.core checkout:dev.tutor checkout:dev.student",
		"clone.all": "run-s clone.native clone.containers clone.core clone.tutor clone.student",
		"fetch.all": "run-s fetch.native fetch.containers fetch.core fetch.tutor fetch.student",
		"install.all": "yarn install.backbone && yarn install.student && yarn install.tutor",
		"install.all.student": "yarn install.backbone && yarn install.student",
		"install.all.tutor": "yarn install.backbone && yarn install.tutor",
		"pull.all": "run-s pull.native pull.containers pull.core pull.tutor pull.student",
		"---- backbone ----------": "echo ---- Backbone  Scripts -----",
		"install.backbone": "run-p install.core install.containers install.native",
		"wml.backbone": "yarn wml.native && yarn wml.containers",
		"---- containers ----------": "echo ---- Containers  Scripts -----",
		"build.containers": "yarn --cwd ./components-containers build",
		"checkout:dev.containers": "echo containers && cd ./components-containers && git checkout dev origin/dev && cd ..",
		"clear:dist.containers": "rm -rf ./components-containers/dist",
		"clear:modules.containers": "rm -rf ./components-containers/node_modules ./components-containers/yarn.lock",
		"clone.containers": "git clone git@github.com:liberedu/components-containers",
		"dev.containers": "yarn --cwd ./components-containers dev",
		"fetch.containers": "cd ./components-containers && git fetch -p",
		"install.containers": "cd ./components-containers && yarn install && cd ..",
		"pull.containers": "cd ./components-containers && git pull",
		"wml.containers": "cd ./components-containers && iml add core",
		"---- core ----------": "echo ---- Core  Scripts -----",
		"build.core": "yarn --cwd ./core build",
		"checkout:dev.core": "echo core && cd ./core && git checkout dev origin/dev && cd ..",
		"clear:dist.core": "rm -rf ./core/dist",
		"clear:modules.core": "rm -rf ./core/node_modules ./core/yarn.lock",
		"clone.core": "git clone git@github.com:liberedu/core",
		"dev.core": "yarn --cwd ./core dev",
		"fetch.core": "cd ./core && git fetch -p",
		"install.core": "cd ./core && yarn install && cd ..",
		"pull.core": "cd ./core && git pull",
		"---- metro ----------": "echo ---- Metro  Scripts -----",
		"clear.metro": "rm -rf /tmp/metro-*",
		"---- native ----------": "echo ---- Native  Scripts -----",
		"build.native": "yarn --cwd ./components-native build",
		"checkout:dev.native": "echo native && cd ./components-native && git checkout dev origin/dev && cd ..",
		"clear:dist.native": "rm -rf ./components-native/dist",
		"clear:modules.native": "rm -rf ./components-native/node_modules ./components-native/yarn.lock",
		"clone.native": "git clone git@github.com:liberedu/components-native",
		"dev.native": "yarn --cwd ./components-native dev",
		"fetch.native": "cd ./components-native && git fetch -p",
		"install.native": "cd ./components-native && yarn install && cd ..",
		"pull.native": "cd ./components-native && git pull",
		"wml.native": "cd ./components-native && iml add core containers",
		"---- student ----------": "echo ---- Student  Scripts -----",
		"checkout:dev.student": "echo student && cd ./student-app && git checkout dev origin/dev && cd ..",
		"clear:dist.student": "rm -rf ./student-app/dist",
		"clear:modules.student": "rm -rf ./student-app/node_modules ./student-app/yarn.lock",
		"clone.student": "git clone git@github.com:liberedu/student-app",
		"fetch.student": "cd ./student-app && git fetch -p",
		"install.student": "cd ./student-app && yarn install && cd ..",
		"pull.student": "cd ./student-app && git pull",
		"wml.student": "cd ./student-app && iml add core containers native",
		"---- tutor ----------": "echo ---- Tutor  Scripts -----",
		"checkout:dev.tutor": "echo tutor && cd ./tutor-app && git checkout dev origin/dev && cd ..",
		"clear:dist.tutor": "rm -rf ./tutor-app/dist",
		"clear:modules.tutor": "rm -rf ./tutor-app/node_modules ./tutor-app/yarn.lock",
		"clone.tutor": "git clone git@github.com:liberedu/tutor-app",
		"fetch.tutor": "cd ./tutor-app && git fetch -p",
		"install.tutor": "cd ./tutor-app && yarn install && cd ..",
		"pull.tutor": "cd ./tutor-app && git pull",
		"wml.tutor": "cd ./tutor-app && iml add core containers native",
		"---- watch ----------": "echo ---- Watch  Scripts -----",
		"clear.watch": "wml rm all && watchman watch-del-all",
		"---- utils ----------": "echo ---- Utils  Scripts -----",
		"build": "yarn tsc",
		"clear": "run-p clear.core clear.native clear.containers clear.student clear.tutor clear.metro clear.watch",
		"containers": "yarn --cwd ./components-containers",
		"core": "yarn --cwd ./core",
		"delay": "node ./node_modules/.bin/npm-delay",
		"dev": "run-p dev.native dev.containers",
		"native": "yarn --cwd ./components-native",
		"student": "yarn --cwd ./student-app",
		"tutor": "yarn --cwd ./tutor-app",
		"update": "git fetch -p && git pull && yarn build && yarn compile"
	},
	"author": "",
	"license": "ISC"
}
